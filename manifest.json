{
  "name": "Pinterest Is Broken",
  "manifest_version": 2,
  "version": "0.1",
  "description": "Removes Pinterest Nag Screen",
  "permissions": ["tabs", "*://*.pinterest.com/*"],
  "content_scripts": [
    {
      "matches": ["*://*.pinterest.com/*"],
      "js": ["pinterest_is_broken.js"]
    }
  ]
}

